		<file value="ElementsCPS.Summary.log" />
		<appendToFile value="true" />
		<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
		<maximumFileSize value="10000KB" />
		<maxSizeRollBackups value="1000" />
		<rollingStyle value="Composite" />
		<layout type="log4net.Layout.PatternLayout">
			<header type="log4net.Util.PatternString" value="&#13;%date&#10;&#13;&#10;Level	UTCDate	Date	Severity	Message	User_Identity	User_Name	User_Web_Identity	Logger	Location	Web_URL	Web_Session_ID	Process_Thread	Machine	Machine_CPU_Count	Machine_OS	Machine_CLR	Process_Memory_Allocated	Process_Memory_Working	Process_User	Process_User_Interactive	Process_Uptime	Exception_Type	Exception_Message	&#13;&#10;" />
			<!--<header value="&#13;&#10;Level	UTCDate	Date	Severity	Message	User_Identity	User_Name	User_Web_Identity	Logger	Location	Web_URL	Web_Session_ID	Process_Thread	Machine	Machine_CPU_Count	Machine_OS	Machine_CLR	Process_Memory_Allocated	Process_Memory_Working	Process_User	Process_User_Interactive	Process_Uptime	Exception_Type	Exception_Message	&#13;&#10;" />-->
			<footer value="&#13;&#10;" />
			<conversionPattern value="%n%-5level	%-25utcdate	%date	%level	%message	%identity	%username	%P{WebUserIdentityName}	%logger	%P{StackLocation}	%P{WebURL}	%P{WebSessionID}	%thread	%P{MachineName}	%P{ProcessorCount}	%P{OSVersion}	%P{CLRVersion}	%P{AllocatedMemory}	%P{WorkingMemory}	%P{ProcessUserDomain}\%P{ProcessUserName}	%P{ProcessUserInteractive}	%timestampms	%P{ExceptionType}	%P{ExceptionMessage}	" />
		</layout>
	</appender>
