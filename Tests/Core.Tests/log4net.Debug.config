	<appender name="Detailed.WindowsEventLog" type="log4net.Appender.EventLogAppender" >
		<applicationName value="ElementsCPS" />
		<mapping>
			<level value="DEBUG" />
			<eventLogEntryType value="Information" />
		</mapping>
		<mapping>
			<level value="INFO" />
			<eventLogEntryType value="Information" />
		</mapping>
		<mapping>
			<level value="WARN" />
