--%>
	<asp:ContentPlaceHolder ID="PortalHeaderArea" runat="server">
	</asp:ContentPlaceHolder>
	<form id="form1" runat="server">
	<asp:ScriptManager ID="ajaxScriptManager" runat="server" 
		OnAsyncPostBackError="ajaxScriptManager_AsyncPostBackError">
		<Scripts>
			<asp:ScriptReference Path="~/scripts/HP.ElementsCPS.Apps.WebUI.js" />
		</Scripts>
	</asp:ScriptManager>
	<!-- HP PORTAL UI : START OUTER TABLE -->
	<table id="HP_PORTAL_UI_OUTER_TABLE" cellspacing="0" cellpadding="0" border="0">
		<!-- HP PORTAL UI : START OUTER TABLE ROW -->
		<tr id="HP_PORTAL_UI_OUTER_TABLE_ROW_NAVBAR_AND_CONTENT" valign="top" align="left">
