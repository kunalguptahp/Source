		</tr>
		<!-- HP PORTAL UI : END OUTER TABLE ROW -->
		<!-- HP PORTAL UI : START OUTER TABLE ROW -->
		<tr id="HP_PORTAL_UI_OUTER_TABLE_ROW_FOOTER" valign="top">
			<td id="HP_PORTAL_UI_OUTER_TABLE_CELL_FOOTER" colspan="3">
				<asp:ContentPlaceHolder ID="PortalFooterArea" runat="server">
				</asp:ContentPlaceHolder>
			</td>
		</tr>
		<!-- HP PORTAL UI : END OUTER TABLE ROW -->
	</table>
	<!-- HP PORTAL UI : END OUTER TABLE -->
